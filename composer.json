{
	"name": "base-mvc-project/php-actions",
	"description": "Example project using php-actions",

	"require-dev": {
        "phpunit/phpunit": "9.*",
        "doctrine/orm": "^2.6.2",
        "symfony/yaml": "2.*",
        "doctrine/dbal": "^2.10",
        "curl/curl": "^2.0",
        "phpstan/phpstan": "0.12.52",
        "monolog/monolog": "2.1.0"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app"
        },
        "psr-0": {"": "app/domain/entities/"}
	},
	"autoload-dev": {
		"psr-4": {
			"App\\Test\\": "tests"
		}
	}
}